{
  "extends": "airbnb",
  "rules": {
    "import/extensions": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": [2, { "ignore": ["children"] }],
    "no-console": ["error", { "allow": ["log", "error"] }],
    "no-multi-spaces": ["error", { "ignoreEOLComments": true }]
  }
}
